{
  "extends": "astro/tsconfigs/strict",
  "references": [{ "path": "../../packages/trpc/tsconfig.json" }],
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "target": "esnext",
    "module": "preserve",
    "moduleResolution": "bundler",
    "strict": true
  }
}
